<Application x:Class="Calculations.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Calculations"
             StartupUri="Main Window/MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="Hack">pack://application:,,,/Resources/#Hack</FontFamily>
        <FontFamily x:Key="HackSymbols">pack://application:,,,/Resources/#Hack Symbols</FontFamily>
        <BitmapImage x:Key="ArrowUpDisabled" UriSource="Resources/ArrowUpDisabled.png"></BitmapImage>
    </Application.Resources>

</Application>
